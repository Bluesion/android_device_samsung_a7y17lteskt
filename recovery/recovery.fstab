/boot		emmc	/dev/block/13540000.dwmmc0/by-name/BOOT
/recovery	emmc	/dev/block/13540000.dwmmc0/by-name/RECOVERY	flags=backup=1
/system		ext4	/dev/block/13540000.dwmmc0/by-name/SYSTEM
/data		ext4	/dev/block/13540000.dwmmc0/by-name/USERDATA	flags=encryptable=footer;length=-20480
/cache		ext4	/dev/block/13540000.dwmmc0/by-name/CACHE
/modem		emmc	/dev/block/13540000.dwmmc0/by-name/RADIO	flags=backup=1;display="Baseband"
/efs		ext4	/dev/block/13540000.dwmmc0/by-name/EFS	        flags=backup=1;display="EFS"
/external_sd	vfat	/dev/block/mmcblk0p1             	        flags=display="Micro SDcard";storage;wipeingui;removable
